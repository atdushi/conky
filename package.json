{
	"name": "conky",
	"displayName": "Conky file support",
	"description": "Add support for .conkyrc files in Linux",
	"icon": "images/conky-manager-icon.png",
	"publisher": "atdushi",
	"repository": {
		"type": "git",
		"url": "https://github.com/atdushi/conky.git"
	},
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "conky",
				"aliases": [
					"Conky",
					"conky"
				],
				"extensions": [
					".conkyrc"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "conky",
				"scopeName": "source.conkyrc",
				"path": "./syntaxes/conky.tmLanguage.json"
			}
		]
	}
}